{
    "manifest_version": 3,
    "name": "LeetCode Challenge Reminder",
    "version": "1.0",
    "description": "Assigns a random LeetCode question every hour.",
    "permissions": ["storage", "tabs", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
    
  }
  